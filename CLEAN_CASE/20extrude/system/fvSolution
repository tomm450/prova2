FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
        solver          GAMG;
        tolerance       1e-09;
        relTol          0.01;
        smoother        GaussSeidel;
        cacheAgglomeration true;
        nCellsInCoarsestLevel 400;
        agglomerator    faceAreaPair;
        mergeLevels     1;
    }

    "(U|k|omega|epsilon)"
    {
        solver          PBiCG;
        preconditioner  DILU;
        tolerance       1e-09;
        relTol          0.1;
    }

}

SIMPLE
{
    nNonOrthogonalCorrectors 	0;
    pRefCell        		0;
    pRefValue       		0;

    residualControl
    {
        p               1e-4;
        U               1e-4;
    }

}

relaxationFactors
{
    fields
    {
    }
    equations
    {
        p           0.3;
        U           0.7;
        k           0.7;
        omega       0.7;
    }
}

