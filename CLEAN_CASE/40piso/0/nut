FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0.14;

boundaryField
{
   outlet
    {
        type            freestream;
        freestreamValue uniform 0.14;
    }
    inlet
    {
        type            freestream;
        freestreamValue uniform 0.14;
    }
    bottom
    {
        type            freestream;
        freestreamValue uniform 0.14;
    }
    top
    {
        type            freestream;
        freestreamValue uniform 0.14;
    }
    airfoil
    {
        type            nutkWallFunction;
        Cmu                0.09;
        kappa            0.41;
        E                9.8;
        value           uniform 0;
    }
}

